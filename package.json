{
  "name": "iconscope",
  "version": "1.0.0",
  "description": "Wrap the Icon with a square(4n) scope",
  "repository": "git@github.com:ziven27/Sketch-IconScope.git",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "IconScope",
    "manifest": "src/manifest.json",
    "main": "plugin.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "author": "ziven27"
}
